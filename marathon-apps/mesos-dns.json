{
  "id": "/mesos-dns",
  "cmd": null,
  "cpus": 0.5,
  "mem": 256,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/config.json",
        "hostPath": "./config.json",
        "mode": "RO"
      }
    ],
    "docker": {
      "image": "tobilg/mesos-dns:v0.5.2",
      "network": "HOST",
      "portMappings": null,
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "env": {
    "LOCAL_IP": "127.0.0.1",
    "MESOS_ZK": "zk://localhost:2181/mesos",
    "MESOS_DNS_EXTERNAL_SERVERS": "8.8.8.8,8.8.4.4"
  },
  "ports": [
    53
  ],
  "uris": [
    "http://localhost:81/mesos-dns/config.json"
  ],
  "args": [
    "/mesos-dns",
    "-config=/config.json"
  ]
}
